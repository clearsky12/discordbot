@bot.event
async def on_reaction_add(reaction, users):
    
    options = webdriver.ChromeOptions()
    options.add_argument("headless")
    driver = webdriver.Chrome(options = options)
    
    if users.bot == 1:
        pass
    else:
        try:
            await Flist.delete()
        except:
            pass
        else:
            if str(reaction.emoji) == '\U0001F4E5':
                await reaction.message.channel.send("ì ì‹œë§Œ ê¸°ë‹¤ë ¤ì£¼ì„¸ìš”. (ì¦ê²¨ì°¾ê¸° ê°¯ìˆ˜ê°€ ë§ìœ¼ë©´ ì§€ì—°ë  ìˆ˜ ìˆìŠµë‹ˆë‹¤.)")
                print(users.name)
                for i in range(len(userFlist)):
                    if userFlist[i][0] == str(users.name):
                        for j in range(1, len(userFlist[i])):
                            try:
                                driver.close()
                            except:
                                print("NOT CLOSED")

                            user.append(userFlist[i][j])
                            result, URLTEST = title(userFlist[i][j])
                            song_queue.append(URLTEST)
                            await reaction.message.channel.send(userFlist[i][j] + "ë¥¼ ì¬ìƒëª©ë¡ì— ì¶”ê°€í–ˆì–´ìš”!")
            elif str(reaction.emoji) == '\U0001F4DD':
                await reaction.message.channel.send("í˜„ì¬ ì§€ì›í•˜ì§€ ì•ŠëŠ” ê¸°ëŠ¥ì…ë‹ˆë‹¤.")
                
# ë©”ì‹œì§€ nì´ˆ í›„ ì‚­ì œí•˜ê¸°

import random

@bot.event
async def on_message(message):
    
    if message.content == "ì•ˆë…•!":
        msg3 = await message.channel.send("ì•ˆë…•í•˜ì„¸ìš”")
        await asyncio.sleep(3)
        await msg3.delete()
        
    if "ë€¨" in message.content:
      await message.delete()
      msg1 = await message.channel.send(f"{message.author.mention} ë‹˜ì´ ê¸ˆì§€ì–´ë¥¼ ì‚¬ìš©í•˜ì˜€ìŠµë‹ˆë‹¤.")
      await asyncio.sleep(5)
      await msg1.delete()
      
    if message.content == "!DM":
        await message.author.send("ì•ˆë…•í•˜ì„¸ìš”!")
        
    if message.content.startswith("!ì²­ì†Œ "):
        purge_number = message.content.replace("!ì²­ì†Œ ", "")
        check_purge_number = purge_number.isdigit()

        if check_purge_number == True:
            await message.channel.purge(limit=int(purge_number) + 1)
            msg = await message.channel.send(f"**{purge_number}ê°œ**ì˜ ë©”ì‹œì§€ë¥¼ ì‚­ì œí–ˆìŠµë‹ˆë‹¤.")
            await asyncio.sleep(5)
            await msg.delete()

        else:
            await message.channel.send("ì˜¬ë°”ë¥¸ ê°’ì„ ì…ë ¥í•´ì£¼ì„¸ìš”.")
    
bot.run(token)
